// 添加数据源
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// 添加生成器
generator client {
  provider = "prisma-client-js"
}

// 添加TokenBlacklist模型
model TokenBlacklist {
  id        Int      @id @default(autoincrement())
  token     String   @unique
  expiresAt DateTime
  createdAt DateTime @default(now())
}

// 添加lastLogoutAt字段到User模型
model User {
  id          Int      @id @default(autoincrement())
  username    String   @unique
  password    String
  fullName    String?
  email       String?
  roles       Json[]
  isActive    Boolean
  lastLogoutAt DateTime? // 新增字段
  createdAt   DateTime  @default(now())
  updatedAt   DateTime  @updatedAt
}

